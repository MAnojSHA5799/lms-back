{
  "version": 2,
  "builds": [
      {
          "src": "package.json",
          "use": "vercel-sapper"
      },
      {
          "src": "Supab.js",
          "use": "@vercel/node"
      }
  ],
  "routes": [
      {
          "src": "/(.*)",
          "dest": "Supab.js",
          "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS"],
          "headers": {
              "Access-Control-Allow-Origin": "*"
          }
      }
  ]
}